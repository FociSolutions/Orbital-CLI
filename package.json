{
  "name": "orb",
  "version": "0.0.0",
  "description": "Orbital CLI ",
  "author": "\"Foci Solutions Inc.\"",
  "bin": {
    "orb": "./bin/run"
  },
  "homepage": "https://github.com/\"Foci Solutions\"/\"Orbital-CLI\"",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "\"Foci Solutions\"/\"Orbital-CLI\"",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "format": "prettier --write .",
    "format:ci": "prettier --check \"*.{json,yml,yaml,md,css}\"",
    "lint": "eslint . --fix --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "prepare": "husky install"
  },
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.0"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "chai": "^4",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "globby": "^13",
    "husky": "^8.0.1",
    "mocha": "^10",
    "oclif": "^3",
    "shx": "^0.3.4",
    "ts-node": "^10.8.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "oclif": {
    "bin": "orb",
    "dirname": "orb",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/\"Foci Solutions\"/\"Orbital-CLI\"/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
